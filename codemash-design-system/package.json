{
  "name": "codemash-design-system",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/codemash-design-system/codemash-design-system.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/",
    "src/scss/"
  ],
  "scripts": {
    "dev": "run-p start storybook",
    "build": "stencil build --docs",
    "serve": "stencil build --dev --watch --serve",
    "start": "stencil build --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@stencil/core": "^2.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/web-components": "^6.4.9",
    "@types/jest": "^26.0.24",
    "babel-loader": "^8.2.3",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "puppeteer": "^10.0.0",
    "autoprefixer": "^10.3.2",
    "css-loader": "^3.6.0",
    "cssnano": "^5.0.8",
    "fs-extra": "^9.0.1",
    "glob-promise": "^3.4.0",
    "lit-html": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.38.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "svgson": "^5.2.1",
    "ts-loader": "^7.0.5"

  },
  "license": "MIT"
}
